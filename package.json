{
  "name": "zeddy-logger",
  "type": "module",
  "version": "1.0.11",
  "description": "Logging library for Node",
  "author": "Zlatibor VeljkoviÄ‡",
  "license": "The Unlicense",
  "homepage": "https://github.com/zveljkovic/zeddy-logging",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zveljkovic/zeddy-logging.git"
  },
  "keywords": [
    "logging",
    "zeddy"
  ],
  "bugs": {
    "url": "https://github.com/zveljkovic/zeddy-logging/issues"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./outputs": {
      "require": "./dist/outputs.cjs",
      "import": "./dist/outputs.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "bumpp": "^10.3.2",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
